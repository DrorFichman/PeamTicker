    <!--    Tutorials-->
    <string name="tutorial_progress">%1$d%%</string>
    <string name="tutorial_headline">תחילת עבודה - הדרכות</string>
    <string name="tutorial_dismiss_message">בטל הודעות הדרכה</string>
    <string name="tutorial_create_player_title">צור שחקנים</string>
    <string name="tutorial_create_player_message">צור שחקנים ידנית דרך אייקון \"+\" בחלק העליון של המסך.\n\nאו \"הדבק שחקנים\" מהודעות WhatsApp בהתבסס על שמות אנשי קשר</string>
    <string name="tutorial_attendance_title">ציין שחקנים משתתפים</string>
    <string name="tutorial_attendance_message">לפני בחירת קבוצות, תצטרך לבחור אילו שחקנים ישתתפו במשחק הקרוב.\n\nסמן את תיבת הסימון המשויכת לכל שחקן משתתף.\n\nתוכל גם להדביק הודעות WhatsApp כדי לאתר שחקנים במהירות.</string>
    <string name="tutorial_start_pick_teams_title">בחר קבוצות</string>
    <string name="tutorial_start_pick_teams_message">תוכל להתחיל לבחור את הקבוצות, פשוט לחץ על כפתור \"קבוצות\" למטה</string>
    <string name="tutorial_pick_teams_title">בחר קבוצות</string>
    <string name="tutorial_pick_teams_message">\"ערבב\" קבוצות בהתבסס על דירוגים.\n(תוכל לבחור מצב ערבוב שונה על ידי לחיצה על 3 הנקודות. לא מומלץ לערבב לפי סטטיסטיקה לפני שיש לך 10 משחקים של נתונים).\n\n\"העבר\" שחקנים על ידי לחיצה ארוכה וגרירה. השתמש בספסל למחליפים.\n\nשתף תמונה של הקבוצות (ללא דירוגים) דרך כל אפליקציית הודעות.</string>
    <string name="tutorial_save_results_title">שמור תוצאת משחק</string>
    <string name="tutorial_save_results_message">ברגע שהמשחק מסתיים, תוכל להזין את התוצאות.\nזה פותח את הדרך להיסטוריית משחקים, סטטיסטיקה אישית, והיכולת לבחור קבוצות בהתבסס על אחוז ניצחונות וכימיה של שחקנים.\n\nהזן \"תוצאות\" במסך \"קבוצות\".</string>
    <string name="tutorial_game_history_title">היסטוריית משחקים</string>
    <string name="tutorial_game_history_message">בחר כל משחק בכרטיסייה \"משחקים\" כדי לצפות בקבוצות שלו ובפעולות.</string>
    <string name="tutorial_cloud_title">תכונות ענן</string>
    <string name="tutorial_cloud_message">סנכרן את השחקנים והמשחקים שלך לענן, כגיבוי, או כשאתה מתכנן למשוך את הנתונים במכשיר אחר.\n\nראשית, התחבר עם חשבון Google שלך דרך התפריט הצדדי.\nלאחר מכן \"סנכרן\" בתפריט הצדדי.</string>
    <string name="tutorial_team_analysis_title">ניתוח קבוצות</string>
    <string name="tutorial_team_analysis_message">עכשיו, שמסופקת היסטוריית משחקים מספקת - לחץ על אייקון \"ניתוח\" בסרגל העליון כדי לצפות באחוזי ניצחון אישיים.\n\nלחץ על שחקן כדי לצפות בכימיה האישית שלו עם שאר השחקנים המשתתפים.</string>
    <string name="tutorial_team_shuffle_stats_title">בחירת קבוצות - מצב AI</string>
    <string name="tutorial_team_shuffle_stats_message">עבור למצב בחירת קבוצות מותאם דרך 3 הנקודות מתחת לכפתור \"ערבב\".\n\nמצב \"סטטיסטיקה\" ייצור קבוצות אופציונליות בהתבסס על אחוזי ניצחונות וכימיה של שחקנים.</string>
    <string name="tutorial_completed_title">הדרכות הושלמו</string>
    <string name="tutorial_completed_message">הדרכות יהיו זמינות בתפריט הצדדי תחת \"תחילת עבודה\"</string>

